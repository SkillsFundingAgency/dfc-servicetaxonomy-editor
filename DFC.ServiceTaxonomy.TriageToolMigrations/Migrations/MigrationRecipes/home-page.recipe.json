              {
                "ContentItemId": "43tx05hwtfw6b5fdxbmdpzbpx0",
                "ContentItemVersionId": null,
                "ContentType": "HTML",
                "DisplayText": "",
                "Latest": false,
                "Published": false,
                "ModifiedUtc": "2024-10-14T15:25:08.3115202Z",
