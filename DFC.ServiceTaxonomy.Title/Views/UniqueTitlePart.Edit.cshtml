@model DFC.ServiceTaxonomy.Title.ViewModels.UniqueTitlePartViewModel

<style asp-name="test" asp-src="~/DFC.ServiceTaxonomy.Title/Styles/titles.min.css" debug-src="~/DFC.ServiceTaxonomy.Title/Styles/titles.css"></style>

<div class="form-group" asp-validation-class-for="Title">
    <label asp-for="Title">
        Title
    </label>
    <p>
        <span class="hint">
            @T["Enter the name of the unique title field."]
        </span>
    </p>
    <input asp-for="Title" class="form-control text-muted" placeholder="e.g. Job Profile Header" />
    <span asp-validation-for="Title"></span>
</div>

