        {
          "ContentItemId": "4bgr5xwt1dceb4j8pd3nfws74p",
          "ContentItemVersionId": "4azjqfn0qj2wqxadq5svc1kcw2",
          "ContentType": "SharedContent",
          "DisplayText": "Exam retakes",
          "Latest": true,
          "Published": true,
          "ModifiedUtc": "2021-04-13T12:46:54.6322034Z",
          "PublishedUtc": "2021-04-13T12:46:55.4723764Z",
          "CreatedUtc": "2021-04-08T15:52:27.377671Z",
          "Owner": "4s69p8vzbjd91zftn97xtkjhxp",
          "Author": "admin",
          "SharedContent": {
            "Content": {
              "Html": "\n<section id=\"exam-retakes\">\n<div class=\"govuk-!-margin-top-6 govuk-!-margin-bottom-6\">\n<h3 class=\"govuk-heading-m\">Exam retakes</h3>\n<ul class=\"govuk-list\">\n    <li class=\"govuk-!-margin-top-3\"><span class=\"govuk-secondary-colour\">Description:&nbsp;</span>If you didn't get the results you wanted, you can resit some of your courses or sign up for some new qualifications.</li>\n    <li><span class=\"govuk-secondary-colour\">Location: </span>School sixth form, sixth form college or further education college</li>\n    <li><span class=\"govuk-secondary-colour\">Duration:&nbsp;</span>1 year</li>\n    <li><span class=\"govuk-secondary-colour\">Entry requirements:&nbsp;</span>16+</li>\n    <li><span class=\"govuk-secondary-colour\">Qualifications: </span><a class=\"govuk-link\" href=\"https://www.gov.uk/what-different-qualification-levels-mean/list-of-qualification-levels\">Level 2 and 3 qualifications</a> like GCSE, A level and BTEC</li>\n    <li><span class=\"govuk-secondary-colour\">Leads to:&nbsp;</span>Higher level or degree apprenticeship, further education, higher education, work </li>\n    <li><span class=\"govuk-secondary-colour\">Find out more: </span> Speak to your school or college careers leader or careers adviser or our careers advisers</li>\n</ul>\n</div>\n</section>"
            }
          },
          "GraphSyncPart": {
            "Text": "<<contentapiprefix>>/sharedcontent/19f4c003-9574-40a5-b2e2-e511d7461ca6"
          },
