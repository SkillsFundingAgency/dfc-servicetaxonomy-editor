                    "SharedContent": {
                        "Content": {
                            "Html": "<div>\n    <p class=\"govuk-body-m\">The National Careers Service offers advice about careers and skills in England. For careers advice in other parts of the UK visit:</p>\n    <ul class=\"govuk-list\">\n        <li><a class=\"govuk-link\" href=\"https://www.myworldofwork.co.uk/learn-and-train/find-free-online-courses/\">Skills Development Scotland - <br>My World of Work</a></li>\n        <li><a class=\"govuk-link\" href=\"https://workingwales.gov.wales/change-your-story/furlough-support\">Careers Wales</a></li>\n        <li><a class=\"govuk-link\" href=\"https://www.nidirect.gov.uk/articles/online-training-support\">Careers Service Northern Ireland</a></li>\n    </ul>\n</div>"
                        }
