<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>netcoreapp3.1</TargetFramework>
      <NoWarn>162</NoWarn>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="CypherToContentRecipes\CleanUpEscoData.recipe.json" />
      <None Remove="CypherToContentRecipes\CreateFullTextSearchIndexesRecipe.json" />
      <None Remove="CypherToContentRecipes\CreateOccupationContentItemsRecipe.json" />
      <None Remove="CypherToContentRecipes\CreateOccupationLabelContentItemsRecipe.json" />
      <None Remove="CypherToContentRecipes\CreateOccupationLabelNodesRecipe.json" />
      <None Remove="CypherToContentRecipes\CreateOccupationPrefLabelNodesRecipe.json" />
      <None Remove="CypherToContentRecipes\CreateSkillContentItems.recipe.json" />
      <None Remove="CypherToContentRecipes\CreateSkillLabelNodesRecipe.json" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="CsvHelper" Version="13.0.0" />
      <PackageReference Include="DotNetCore.NPOI" Version="1.2.2" />
      <PackageReference Include="Microsoft.AspNet.WebApi.Client" Version="5.2.7" />
      <PackageReference Include="Microsoft.AspNetCore.WebUtilities" Version="2.2.0" />
      <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="3.1.2" />
      <PackageReference Include="morelinq" Version="3.3.2" />
      <PackageReference Include="OrchardCore.Infrastructure" Version="1.0.0-rc1-*" />
    </ItemGroup>

    <ItemGroup>
      <None Update="appsettings.Development.json">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <Content Include="CypherToContentRecipes\*.recipe.json">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
      <Content Include="ContentRecipes\*.recipe.json">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
      <None Update="SeedData\*">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\DFC.ServiceTaxonomy.GraphSync\DFC.ServiceTaxonomy.GraphSync.csproj" />
    </ItemGroup>

</Project>
