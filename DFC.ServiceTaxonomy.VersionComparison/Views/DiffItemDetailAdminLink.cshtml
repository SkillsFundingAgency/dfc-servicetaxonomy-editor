@model DiffItem
<div class="container-fluid">
    <div class="row diff">
        <div class="col-3">
            <span class="diff-item-label">@Model.Name</span>
        </div>
        <div class="col-4 base">
            @if (Model.CompareURLs != null)
            {
                foreach (var url in Model.CompareURLs)
                {
                    <div>
                        <span>@url.Value</span>
                    </div>
                }
            }
        </div>
        <div class="col-4 compare">
            @if (Model.BaseURLs != null)
            {
                foreach (var url in Model.BaseURLs)
                {
            <div>
                <span>@url.Value</span>
            </div>
                }
            }
        </div>
    </div>
</div>
