                  "Html": "\n \n\n\n\n<div class=\"image-container\">\n <div class=\"banner-d-only\">\n                        <img src=\"https://cdn.nationalcareers.service.gov.uk/media/homepage-featured-banner/seasonal-jobs-2024-featured-desktop.png\" id=\"banner-img-seasonal-jobs\" alt=\"Seasonal work\">\n</div>\n <div class=\"banner-m-only\">\n                        <img src=\"https://cdn.nationalcareers.service.gov.uk/media/homepage-featured-banner/seasonal-jobs-2024-featured-mobile.png\" id=\"banner-img-seasonal-jobs-m\" alt=\"Seaosonal work\">\n</div>\n\n\n                        <div class=\"overlay-content-header overlay-header\">\n                            <h2>Seasonal work advice</h2>                                                    \n                        </div>\n                    <div class=\"overlay-content\">\n                        \n                            <span>Get advice on how to find part time or full time opportunities over Christmas with our <a class=\"govuk-link\" id=\"banner-link-green-careers\" href=\"/seasonal-work\">\n seasonal work advice</a>.</span>\n                        \n                    </div>\n                </div>\n      \n     \n    "
